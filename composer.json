{
    "name": "phpviet/yii-validation",
    "type": "yii2-extension",
    "description": "Yii validation hổ trợ kiểm tra dữ liệu đặc thù trong nước",
    "keywords": [
        "phpviet",
        "yii-validation"
    ],
    "homepage": "https://github.com/phpviet/yii-validation",
    "license": "MIT",
    "authors": [
        {
            "name": "Vuong Xuong Minh",
            "email": "vuongxuongminh@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpviet/validation": "^1.0",
        "yiisoft/yii2": "~2.0.13"
    },
    "autoload": {
        "psr-4": {
            "phpviet\\yii\\validation\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "bootstrap": "phpviet\\yii\\validation\\Bootstrap"
	}
}
